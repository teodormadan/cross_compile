build:
    aarch64-docker: {}
    aarch64-linux:
        cmake-args:
            - "-DCMAKE_TOOLCHAIN_FILE=/toolchains/aarch64-gnu.cmake"
            - "--no-warn-unused-cli"
    armhf-docker:
        cmake-args:
            - "-DCMAKE_C_FLAGS=-Wno-psabi"
            - "-DCMAKE_CXX_FLAGS=-Wno-psabi"
            - "--no-warn-unused-cli"
    armhf-linux:
        cmake-args:
                "--no-warn-unused-cli"
            ],
        }
    }
}
